<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>



<!-- FORM -->
		<record model="ir.ui.view" id="slot_form_view">
			<field name="name">matrix.slot.form</field>
			<field name="model">matrix.slot</field>
			<field name="arch" type="xml">
				<form string="slot Form">
					<sheet>
						<group>
							<group>
						        <field name="patient"/>
						        <field name="doctor"	readonly="1"/>
<!--
						        <field name="appointment"/>
-->
							</group>
							<group>
								<field name="date_start"      widget="float_time"	readonly="1"/>
						        <field name="date"	readonly="1"/>
							</group>
						</group>

						<group>
							<group>
<!--								
						        <field name="container_id"/>

						        <field name="container_0_id"/>
						        <field name="container_1_id"/>
						        <field name="container_2_id"/>

						        <field name="container_3_id"/>
						        <field name="container_4_id"/>
						        <field name="container_5_id"/>
-->

						        <field name="slot_type"/>
						        <field name="state"/>
							</group>
							<group>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>





<!-- TREE -->
		<record model="ir.ui.view" id="slot_view_tree">
			<field name="name">matrix.slot.tree</field>
			<field name="model">matrix.slot</field>
			<field name="arch" type="xml">
				<tree string="slot Tree">
<!--
			        <field name="date"/>
			        <field name="name"/>
-->
					<field name="date_start"    widget="float_time"				/>

			        <field name="patient"/>
				</tree>
			</field>
		</record>





